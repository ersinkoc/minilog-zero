{
  "name": "minilog-zero",
  "version": "1.0.0",
  "description": "Zero-dependency colorful console logger with levels, timestamps and prefixes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node dist/test/index.js",
    "example:basic": "npm run build && node dist/examples/basic.js",
    "example:custom": "npm run build && node dist/examples/custom-logger.js",
    "example:levels": "npm run build && node dist/examples/level-filtering.js",
    "example:objects": "npm run build && node dist/examples/objects.js",
    "examples": "npm run example:basic && npm run example:custom && npm run example:levels && npm run example:objects",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logger",
    "console",
    "zero-dependency",
    "colorful",
    "typescript",
    "nodejs",
    "logging",
    "debug"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ersinkoc/minilog-zero.git"
  },
  "bugs": {
    "url": "https://github.com/ersinkoc/minilog-zero/issues"
  },
  "homepage": "https://github.com/ersinkoc/minilog-zero#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.3.0"
  }
}
